@{
    ViewBag.Title = "Contact";
}

<div class="row">

    <div class="col-md-8">
        <h4>Contact Us</h4><br />
        <div class="form-horizontal">
            <br /><br />
            @using (Html.BeginForm())
            {
                <div class="form-group">
                    <label for="uname" class=" col-md-2">Name</label>
                    <div class="col-md-5">
                        <input type="text" class="form-control" name="uname" id="uname" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="email" class=" col-md-2">Email</label>
                    <div class="col-md-5">
                        <input type="text" class="form-control" name="email" id="email" />
                    </div>
                </div>
                <div class="form-group">
                    <label for="email" class=" col-md-2">Comment</label>
                    <div class="col-md-5">
                        <input type="text" class="form-control" name="email" id="email" />
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Submit" class="btn btn-primary" />
                    </div>
                </div>
            }
            <form></form>
        </div>
    </div>
    <div class="col-md-4">
        <br />
        <h4>Address</h4>
        <address>
            One Microsoft Way<br />
            Redmond, WA 98052-6399<br />
            <abbr title="Phone">P:</abbr>
            425.555.0100
        </address>
        <address>
            <strong>Support:</strong>   <a href="mailto:Support@example.com">Support@greenpro.com</a><br />
            <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@greenpro.com</a>
        </address>
    </div>
    <div class="col-md-12" style="text-align:center">
        <h4>Locate Us</h4><br />

        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d387157.48218081944!2d-73.97968099999999!3d40.703312749999995!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c24fa5d33f083b%3A0xc80b8f06e177fe62!2sNew+York%2C+NY%2C+USA!5e0!3m2!1sen!2sin!4v1431595635973" class="col-md-12" height="350" frameborder="0" style="border:0"></iframe>

    </div>


</div><br /><br />

<script>
    $(function () {
        $('#homeMenu').removeClass('current-menu-item');
        $('#serviceMenu').removeClass('current-menu-item');
        $('#garagesMenu').removeClass('current-menu-item');
        $('#aboutMenu').removeClass('current-menu-item');
        $('#contactMenu').addClass('current-menu-item');
    })
</script>
