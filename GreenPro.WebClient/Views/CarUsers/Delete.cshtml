@model GreenPro.Data.CarUser

@{
    ViewBag.Title = "Delete";
    Layout = null;
}


<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 class="modal-title">Delete Car</h3>
</div>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="modal-body">
        <div class="form-group">
            <div class="col-md-10">
                <h4>
                    <span class="label label-danger">
                        @ViewBag.Message
                    </span>
                </h4>
            </div>
            <br />
        </div>
    </div>
    <div class="modal-footer">
        <button class="line-btn light" data-dismiss="modal">No</button>
        @*<input class="line-btn light" type="submit" value="Yes" disabled="@ViewBag.Disabled" />*@
        <input class="line-btn light" type="submit" value="Yes" />
    </div>
}

